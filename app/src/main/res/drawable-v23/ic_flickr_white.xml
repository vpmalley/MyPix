<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="10dp"
        android:height="10dp"
        android:start="2dp"
        android:top="7dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="oval">
            <solid
                android:color="@android:color/white"/>
        </shape>
    </item>
    <item
        android:width="10dp"
        android:height="10dp"
        android:start="12dp"
        android:top="7dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
               android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>